{
  "name": "@mcph/miix-webpack-plugin",
  "version": "0.1.1",
  "description": "Webpack plugin for Mixer interactive projects",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "npm-run-all --parallel --silent test:lint test:unit fmt",
    "test:unit": "mocha",
    "test:lint": "tslint --project tsconfig.json '{src,test}/**/*.ts'",
    "fmt": "npm run -s fmt:ts && npm run test:lint -- --fix",
    "fmt:ts": "prettier --single-quote --trailing-comma all --parser typescript --print-width 100 --write \"{src,test}/**/*.ts\"",
    "build": "tsc",
    "prepare": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mixer/miix-webpack-plugin.git"
  },
  "keywords": [
    "miix",
    "mixer",
    "interactive",
    "controls",
    "webpack",
    "plugin"
  ],
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixer/miix-webpack-plugin/issues"
  },
  "homepage": "https://github.com/mixer/miix-webpack-plugin#readme",
  "dependencies": {
    "glob": "^7.1.2",
    "joi": "^13.1.2",
    "json5": "^1.0.1",
    "marked": "^0.3.19",
    "parse5": "^4.0.0",
    "tar": "^4.4.1",
    "typescript": "~2.7.2"
  },
  "devDependencies": {
    "@mcph/miix-std": "^0.2.12",
    "@types/chai": "^4.1.2",
    "@types/chai-subset": "^1.3.1",
    "@types/glob": "^5.0.35",
    "@types/joi": "^13.0.7",
    "@types/json5": "0.0.29",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "@types/rimraf": "^2.0.2",
    "@types/tar": "^4.0.0",
    "@types/webpack": "^4.1.2",
    "chai": "^4.1.2",
    "chai-subset": "^1.6.0",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  }
}
